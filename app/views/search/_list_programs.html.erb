
	<table id="tableData2" class="hover row-border" style="margin-top:70px; margin-bottom:40px;">
		<thead>
			<tr>
				<th>
					<center><font color="BLACK"><strong>Nome do Programa</strong></font></center>
				</th>
				<th>
					<center><font color="BLACK"><strong>Gasto Total no Programa (R$)</strong></font></center>
				</th>
			</tr>
		</thead>
		<tbody>
		<% entity = 0
		   expense = 1
		@results[:program].each do |program| %>
		  	<tr onclick="location.href='<%= program_path(program[entity].id) %>'" style="cursor: pointer;">
			  	<td>
			  		<center><%= program[entity].name%></center>
			  	</td>
			  	<td>	
			  		<center><%= program[expense] %></center>
			  	</td>
		  	</tr>
		<% end %>
		</tbody>
	</table>
