<div style="color: #999999;" id="tableLocation">
<table id="tableData1" class="hover row-border" style="background:#333333;margin-top:70px; margin-bottom:40px;">
		<thead>
			<tr>
				<th>
					<center><font color="white"><strong>Entidade</strong></font></center>
				</th>
				<th>
					<center><font color="white"><strong>Nome do Órgão</strong></font></center>
				</th>
				<th>
					<center><font color="white"><strong>Gasto Total(R$)</strong></font></center>
				</th>
				
			</tr>
		</thead>
		<tbody>
		<% entity_idx = 0
		   expense = 1
		@results.each do |entity,entity_find| 
			name = ""
			if entity == :company
				name = "/company/"
			elsif entity == :agency
				name = "/public_agency/"
			else
				name = "/program/"
			end 
			entity_find.each do |entity_object|%>
			
			  	<tr onclick="location.href='<%=  name+entity_object[entity_idx].id.to_s %>'" 
			  	style="cursor: pointer;">
				  	<td>
				  	<center><%= name %></center>
				  	</td>
				  	<td>
				  		<center><%= entity_object[entity_idx].name %></center>
				  	</td>
				  	<td>	
				  		<center><%= entity_object[expense] %></center>
				  	</td>
			  	</tr>
			<% end %>
		<% end %>
		</tbody>
	</table>
</div>